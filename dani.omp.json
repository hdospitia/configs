{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
      {
        "alignment": "left",
        "segments": [
          {
            "foreground": "#306C85",
            "properties": {
              "time_format": "15:05"
            },
            "style": "plain",
            "template": "󰥔 {{ .CurrentDate | date .Format }} ",
            "type": "time"
          },
          {
            "foreground": "#306C85",
            "style": "plain",
            "template": "󰇝 {{ .UserName }} ",
            "type": "session"
          },
          {
            "foreground": "cyan",
            "properties": {
              "style": "agnoster_full"
            },
            "style": "plain",
            "template": "<#306C85>in</> {{ .Path }} ",
            "type": "path"
          },
          {
            "foreground": "#AC0E0E",
            "properties": {
              "branch_icon": " <#AC0E0E><b>\ue0a0 </b></>",
              "fetch_stash_count": true
            },
            "style": "plain",
            "template": "<#A97A4B>on</> {{ .HEAD }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }} ",
            "type": "git"
          },
          {
            "type": "text",
            "style": "plain",
            "foreground": "#FF9900",
            "template": "{{ if .Env.AWS_PROFILE }} 󰇝 󰸏 {{ .Env.AWS_PROFILE }} {{ end }}"
          },
          {
            "type": "session",
            "style": "plain",
	    "foreground": "#55531C",
            "template": "{{ if .SSHSession }} {{ .UserName }} in {{ .HostName }} {{ end }}"
          },
          {
            "type": "executiontime",
            "style": "plain",
            "foreground": "#306C85",
            "template": "󰇝 󱦟 {{ .FormattedMs }} ",
            "properties": {
              "threshold": 500,
              "style": "austin",
              "always_enabled": true
            }
          }
        ],
        "type": "prompt"
      },
      {
        "alignment": "left",
        "newline": true,
        "segments": [
          {
            "foreground": "lightGreen",
            "style": "plain",
            "template": "\u276f",
            "type": "text"
          }
        ],
        "type": "prompt"
      }
    ],
    "final_space": true,
    "version": 2
  }
